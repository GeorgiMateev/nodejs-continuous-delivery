  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"node server.js\" ",
    "e2e": "tsc && concurrently \"node server.js\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./public/app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "npm run-script unit && tsc && concurrently \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "unit": "./node_modules/.bin/mocha --recursive -R xunit test/ > test-reports.xml",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
